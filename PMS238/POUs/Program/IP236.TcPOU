      <Get Name="Get" Id="{f04f0807-29d0-4fd1-98f0-6dbddec742e2}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[Busy := nState <> STATE_WAIT_CMD; ]]></ST>
